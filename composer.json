{
  "name": "insidion/swagger-bundle",
  "description": "Generate Swagger documentation for Symfony 3+ projects",
  "keywords": [
    "bundle",
    "api",
    "doc",
    "swagger"
  ],
  "type": "symfony-bundle",
  "homepage": "https://github.com/PHPSocialNetwork/InsidionSwaggerBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Georges.L",
      "email": "contact@geolim4.com",
      "homepage": "https://github.com/Geolim4",
      "role": "Owner / Current maintainer"
    },
    {
      "name": "Mitchell Herrijgers",
      "role": "Former maintainer / Original creator"
    }
  ],
  "require": {
    "php": ">= 5.6",
    "symfony/filesystem": "^3.2",
    "symfony/framework-bundle": "^3.2",
    "psr/cache": "^1.0",
    "symfony/routing": "^3.2",
    "doctrine/annotations": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "symfony/phpunit-bridge": "^4.0"
  },
  "suggest": {
    "phpfastcache/phpfastcache": "^5.0 || ^6.0"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Insidion\\SwaggerBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Insidion\\SwaggerBundle\\Tests\\": "tests/"
    }
  }
}
